{
  "expo": {
    "name": "Aurashop",
    "slug": "aurashop",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "https://i.pinimg.com/736x/1d/b8/0e/1db80e668536283ff38d29f7b47d8ec3.jpg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "https://i.pinimg.com/736x/1d/b8/0e/1db80e668536283ff38d29f7b47d8ec3.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#161622"
    },
    "ios": {
      "bundleIdentifier": "com.itgenius.aurashop",
      "supportsTablet": true,
      "userInterfaceStyle": "automatic",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to let you upload profile photos.",
        "NSPhotoLibraryUsageDescription": "This app uses the photo library to let you upload profile photos."
      }
    },
    "android": {
      "package": "com.itgenius.aurashop",
      "versionCode": 1,
      "ndkVersion": "26.0.10792818",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-localization",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends.",
          "cameraPermission": "The app accesses your camera to let you share photos with your friends."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "8986c377-6551-48c0-b9dd-1c5bcd11a307"
      }
    },
    "owner": "attitaya"
  }
}
